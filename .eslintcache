\\src\\components\\header\\header.js",["89","90","91","92"],"import React from 'react';\r\nimport './header.css';\r\n\r\nconst Header = () => {\r\n    return (\r\n    <div className=\"header\">\r\n        <h3><a href=\"#\">Star-DB</a></h3>\r\n        <ul>\r\n            <li><a href=\"#\">People</a></li>\r\n            <li><a href=\"#\">Planets</a></li>\r\n            <li><a href=\"#\">Starships</a></li>\r\n        </ul>\r\n    </div>\r\n        \r\n\r\n    )\r\n}\r\n\r\nexport default Header;\r\n",["93","94"],"C:\\Users\\fssro\\react-apps\\star-db\\src\\components\\random-planet\\random-planet.js",["95"],"import React, {Component} from 'react';\r\nimport Spinner from '../spinner';\r\nimport './random-planet.css';\r\nimport SwapiService from '../../services/swapi-service.js';\r\nimport './random-planet.css';\r\nimport ErrorIndicator from '../error-indicator';\r\n\r\nexport default class RandomPlanet extends Component {\r\n\r\n    swapi = new SwapiService();\r\n\r\n    constructor() {\r\n        super();\r\n        \r\n    }\r\n\r\n    componentDidMount() {\r\n        this.updatePlanet();\r\n        this.interval = setInterval(this.updatePlanet, 10000)\r\n    }\r\n\r\n    state = {\r\n        planet: {},\r\n        loading: true,\r\n        error: false\r\n    };\r\n\r\n    onPlanetLoaded = (planet) => {\r\n        this.setState({\r\n            planet,\r\n            loading: false,\r\n            error: false\r\n        });\r\n    }\r\n\r\n    onError = () => {\r\n        this.setState({error: true, loading: false})\r\n    }\r\n\r\n    updatePlanet() {\r\n        const id = Math.floor(Math.random() * 25 + 2);\r\n        this.swapi.getPlanet(id)\r\n        .then(this.onPlanetLoaded)\r\n        .catch(this.onError)\r\n    }\r\n    render() {\r\n\r\n        \r\n        const {planet, loading, error} = this.state;\r\n        const spinner = loading ? <Spinner/> : null;\r\n        const content = ! (loading || error) ? <PlanetView planet={planet}/> : null;\r\n        const errorBlock = error ? <ErrorIndicator/> : null;\r\n\r\n        return(\r\n            <div className=\"block-center\">\r\n                { spinner }\r\n                { errorBlock }\r\n                { content }\r\n            </div>\r\n        )\r\n    }\r\n};\r\n\r\nconst PlanetView = ({planet}) => { \r\n    const { id , name, population, rotationPeriod, diameter } = planet;\r\n    return (\r\n        <>\r\n            <img src={`https://starwars-visualguide.com/assets/img/planets/${id}.jpg`} alt='planet_img'/>\r\n            <div className=\"random-planet__name\">\r\n                    <h4><span>{name}</span></h4>\r\n                <ul>\r\n                    <li><span>Population </span><span>{population}</span></li>\r\n                    <li><span>Rotation Period </span><span>{rotationPeriod}</span></li>\r\n                    <li><span>Diameter </span><span>{diameter}</span></li>\r\n                </ul>\r\n            </div>     \r\n        </>\r\n    )\r\n}\r\n\r\n\r\n    \r\n        \r\n    \r\n\r\n","C:\\Users\\fssro\\react-apps\\star-db\\src\\components\\app\\index.js",[],"C:\\Users\\fssro\\react-apps\\star-db\\src\\components\\spinner\\spinner.js",[],"C:\\Users\\fssro\\react-apps\\star-db\\src\\components\\error-indicator\\error-indicator.js",[],"C:\\Users\\fssro\\react-apps\\star-db\\src\\services\\swapi-service.js",[],"C:\\Users\\fssro\\react-apps\\star-db\\src\\components\\random-planet\\index.js",[],"C:\\Users\\fssro\\react-apps\\star-db\\src\\components\\header\\index.js",[],"C:\\Users\\fssro\\react-apps\\star-db\\src\\components\\person-details\\index.js",[],"C:\\Users\\fssro\\react-apps\\star-db\\src\\components\\person-details\\person-details.js",[],"C:\\Users\\fssro\\react-apps\\star-db\\src\\components\\item-list\\item-list.js",[],"C:\\Users\\fssro\\react-apps\\star-db\\src\\components\\item-list\\index.js",[],"C:\\Users\\fssro\\react-apps\\star-db\\src\\components\\spinner\\index.js",[],"C:\\Users\\fssro\\react-apps\\star-db\\src\\compo