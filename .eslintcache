 const content = ! (loading || error) ? <PlanetView planet={planet}/> : null;\r\n        const errorBlock = error ? <ErrorIndicator/> : null;\r\n\r\n        return(\r\n            <div className=\"block-center\">\r\n                { spinner }\r\n                { errorBlock }\r\n                { content }\r\n            </div>\r\n        )\r\n    }\r\n};\r\n\r\nconst PlanetView = ({planet}) => { \r\n    const { id , name, population, rotationPeriod, diameter } = planet;\r\n    return (\r\n        <>\r\n            <img src={`https://starwars-visualguide.com/assets/img/planets/${id}.jpg`} alt='planet_img'/>\r\n            <div className=\"random-planet__name\">\r\n                    <h4><span>{name}</span></h4>\r\n                <ul>\r\n                    <li><span>Population </span><span>{population}</span></li>\r\n                    <li><span>Rotation Period </span><span>{rotationPeriod}</span></li>\r\n                    <li><span>Diameter </span><span>{diameter}</span></li>\r\n                </ul>\r\n            </div>     \r\n        </>\r\n    )\r\n}\r\n\r\n\r\n    \r\n        \r\n    \r\n\r\n","C:\\Users\\fssro\\react-apps\\star-db\\src\\components\\app\\index.js",[],["130","131"],"C:\\Users\\fssro\\react-apps\\star-db\\src\\components\\spinner\\spinner.js",[],"C:\\Users\\fssro\\react-apps\\star-db\\src\\components\\error-indicator\\error-indicator.js",[],"C:\\Users\\fssro\\react-apps\\star-db\\src\\services\\swapi-service.js",[],"C:\\Users\\fssro\\react-apps\\star-db\\src\\components\\random-planet\\index.js",[],["132","133"],"C:\\Users\\fssro\\react-apps\\star-db\\src\\components\\header\\index.js",[],"C:\\Users\\fssro\\react-apps\\star-db\\src\\components\\item-list\\item-list.js",[],"C:\\Users\\fssro\\react-apps\\star-db\\src\\components\\item-list\\index.js",[],"C:\\Users\\fssro\\react-apps\\star-db\\src\\components\\spinner\\index.js",[],"C:\\Users\\fssro\\react-apps\\star-db\\src\\components\\error-indicator\\index.js",[],"C:\\Users\\fssro\\react